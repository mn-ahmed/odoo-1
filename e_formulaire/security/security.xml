<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model="ir.module.category" id="e_formulaire_category">
        <field name="name">E-Formulaire</field>
        <field name="sequence">10</field>
    </record>

    <record id="group_e_formulaire_user" model="res.groups">
    <field name="name">Responsible</field>
    <field name="category_id" ref="e_formulaire_category"/>
    <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_e_formulaire_responsibler" model="res.groups">
        <field name="name">Tout Approbation</field>
        <field name="category_id" ref="e_formulaire_category"/>
        <field name="implied_ids" eval="[(4, ref('e_formulaire.group_e_formulaire_user'))]"/>
    </record>

    <record id="group_e_formulaire_manager" model="res.groups">
        <field name="name">Administrateur</field>
        <field name="category_id" ref="e_formulaire_category"/>
        <field name="implied_ids" eval="[(4, ref('e_formulaire.group_e_formulaire_responsibler'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

    <data noupdate="0">


    </data>
</odoo>